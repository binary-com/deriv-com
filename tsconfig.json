{
    "include": ["./src/**/*", "./src/custom.d.ts"],
      "jsx": "react",
      "compilerOptions": {
        "baseUrl": "src",
        "jsx": "preserve",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noEmit": true,
        "skipLibCheck": true,
        "moduleResolution": "node"
      }
}
