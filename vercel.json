{
    "rewrites": [
        {
            "source": "/proxy/*",
            "has": [
                {
                    "type": "query",
                    "key": "url",
                    "value": "(?<pathname>.*)"
                  }
                ],
            "destination": "https://www.googletagmanager.com/:pathname" 
        }
    ],
    "headers": [
        {
            "source": "/proxy/*",
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ]
        }
    ]
}