{
    "headers": [
        {
            "source": "/party-proxy",
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/party-proxy",
            "has": [
                {
                    "type": "query",
                    "key": "url",
                    "value": "(?<url>.*)"
                }
            ],
            "destination": ":url" 
        }
    ]
}